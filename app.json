{
  "expo": {
    "name": "Hubiptv",
    "slug": "Hubiptv",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/logo_black_back_white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "platforms": [
      "android"
    ],
    "android": {
      "package": "com.hubiptv.app"
    }
  }
}
